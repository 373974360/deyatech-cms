        <result column="to_dept_id" property="toDeptId" />
        <result column="pro_time" property="proTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id_,
        enable_,
        remark_,
        create_by,
        create_time,
        update_by,
        update_time,
        version_,
        sq_id, pro_type, pro_content, reasons_delay, reasons_time, pro_dept_id, to_dept_id, pro_time
